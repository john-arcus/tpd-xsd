                    <xs:documentation>Yes, only voltage adjustable</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="ONLY_VOLTAGE_ADJUSTABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Yes, only wattage adjustable</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="ONLY_WATTAGE_ADJUSTABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>No, un-adjustable</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="UNADJUSTABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="Submission" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract submission.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Submitter" type="Submitter">
                <xs:annotation>
                    <xs:documentation>The submitter information.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SubmissionType" type="SubmissionType">
                <xs:annotation>
                    <xs:documentation>The type of the submission.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GeneralComment" type="String1000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>General comments to accompany notification. When modifying/updating an existing notification, please
                        indicate here which information is being changed.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SubmissionResponse" abstract="true">
