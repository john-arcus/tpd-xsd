                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="VOLTAGE_WATTAGE_ADJUSTABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
