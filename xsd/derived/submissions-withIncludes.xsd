    <xs:complexType name="TncoEmission">
        <xs:annotation>
            <xs:documentation>Defines a TNCO emission.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Tar" type="Double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The tar yield according to the ISO standard 4387 with the accuracy measurements determined in
                        accordance with ISO standard 8243.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Nicotine" type="Double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The nicotine yield according to the ISO standard 10315 with the accuracy measurements determined in
                        accordance with ISO standard 8243.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Co" type="Double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The carbon monoxide yield according to the ISO standard 8454 with the accuracy measurements determined
                        in accordance with ISO standard 8243.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Laboratories" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The identification of the laboratory/laboratories used to measure emissions of tar, nicotine and
                        carbon monoxide.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Laboratory" type="String300" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TobaccoEmission">
        <xs:annotation>
            <xs:documentation>Defines a tobacco product emission.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Emission">
                <xs:sequence>
                    <xs:element name="Name" type="String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The chemical name of the emission(s) produced during the testing of the product.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CigaretteSpecific">
        <xs:annotation>
            <xs:documentation>Defines the specific information for cigarette products.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CharacterisingFlavour" type="Boolean">
                <xs:annotation>
                    <xs:documentation>The classification of the cigarette as having a characterising flavour as referred to in Article 7(14)
                        of Directive 2014/40/EU.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FilterVentilation" type="Percentage">
                <xs:annotation>
                    <xs:documentation>The total ventilation of the filter (0-100%).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FilterDropPressureClosed" type="Double">
                <xs:annotation>
                    <xs:documentation>The drop of pressure with closed vents (mmH2O).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FilterDropPressureOpen" type="Double">
                <xs:annotation>
                    <xs:documentation>The drop of pressure with open vents (mmH2O).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SmokelessSpecific">
        <xs:annotation>
            <xs:documentation>Defines the specific information for smokeless products.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RyoPipeSpecific">
                <xs:sequence>
                    <xs:element name="Ph" type="Double">
                        <xs:annotation>
                            <xs:documentation>The pH of the product.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="TotalMoisture" type="Double" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The total moisture of the product per product unit.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="AnalysisMethods" type="String300" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The brief Text description of the methods used to measure the above variables including
                                references to more detailed information on the approach.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RyoPipeSpecific">
        <xs:annotation>
            <xs:documentation>Defines specific information for Roll-your-own / Pipe products.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TotalNicotineContent" type="Double">
                <xs:annotation>
                    <xs:documentation>The total nicotine content of the product per product unit.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UnionisedNicotineContent" type="Double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The unionised nicotine content of the product per product unit.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="NovelSpecific">
        <xs:annotation>
            <xs:documentation>Defines specific information for novel tobacco product.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="DetailsDescriptionFile" type="AttachmentRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A PDF file describing the available scientific studies on toxicity, addictiveness and attractiveness
                        of the novel tobacco product, in particular as regards its ingredients and emissions.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UseInstructionsFile" type="AttachmentRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A PDF file describing the available studies, executive summaries thereof and market research on the
                        preferences of various consumer groups, including young people and current smokers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RiskBenefitFile" type="AttachmentRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The pdf file describing the other available and relevant information, including a risk/benefit
                        analysis of the product, its expected effects on cessation of tobacco consumption, its expected effects on
                        initiation of tobacco consumption and predicted consumer perception.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StudyFiles" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Other available studies/information as provided for under Article 19 of the TPD and not submitted in
                        other sections.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Attachment" type="AttachmentRef" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    

    <xs:complexType name="EcigProduct">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette product.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Product">
                <xs:sequence>
                    <xs:element name="ProductType" type="EcigProductType">
                        <xs:annotation>
                            <xs:documentation>The e-cigarette product type.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Weight" type="Double" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Total weight of e-liquid in one product unit in mg.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Volume" type="Double" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Total weight of e-liquid in one product unit in ml.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ClpClassification" type="String300" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Overall product classification (including labelling elements) as a mixture of substances based
                                on Regulation (EC) No 1272/2008 of the European Parliament and of the Council and as described in the
                                "Guidance on the Application of the CLP Criteria".
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="StudySummaryFiles" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Executive summaries of any market studies carried out in respect of the above fields (Sales
                                Volume, Mode of Sales, Market Research), including an English translation thereof
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Attachment" type="AttachmentRef" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Presentations">
                        <xs:annotation>
                            <xs:documentation>Description of the format(s) and Member State(s) in which the product is presented for sale.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Presentation" type="EcigPresentation" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Ingredients" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The list of product ingredients.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Ingredient" type="EcigIngredient" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Emissions" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The list of product emissions.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Emission" type="EcigEmission" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Design" type="Design">
                        <xs:annotation>
                            <xs:documentation>The product design information.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="EcigProductType">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette product type.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="EcigProductTypeEnum">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the product type is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="EcigProductTypeEnum">
        <xs:annotation>
            <xs:documentation>The list of e-cigarette product types.</xs:documentation>
            <xs:appinfo>
                <jaxb:typesafeEnumClass/>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Electronic cigarette – Disposable.</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="DISPOSABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Electronic cigarette – Rechargeable, placed on the market with one type of e-liquid (fixed
                        combination). Any rechargeable which can also be used as a refillable should be reported under the refillable
                        category.
                    </xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="RECHARGEABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Electronic cigarette – Rechargeable, device only Any rechargeable which can also be used as a
                        refillable should be reported under the refillable category.
                    </xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="RECHARGEABLE_DEVICE_ONLY"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Electronic cigarette – Refillable, placed on the market with one type of e-liquid (fixed
                        combination).
                    </xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="REFILLABLE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>Electronic cigarette – Refillable, device only.</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="REFILLABLE_DEVICE_ONLY"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation>Kit – Pack containing more than one different e-cigarette device and/or more than one different refill
                        container/cartridge.
                    </xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="KIT"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="7">
                <xs:annotation>
                    <xs:documentation>Refill container/cartridge containing e-liquid.</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="REFILL_CONTAINER_CARTRIDGE"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="8">
                <xs:annotation>
                    <xs:documentation>Individual part of electronic cigarette capable of containing e-liquid.</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="INDIVIDUAL_PART"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="9">
                <xs:annotation>
                    <xs:documentation>Other</xs:documentation>
                    <xs:appinfo>
                        <jaxb:typesafeEnumMember name="OTHER"/>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="EcigPresentation">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette product presentation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Presentation">
                <xs:sequence>
                    <xs:element name="PackageUnits" type="Integer">
                        <xs:annotation>
                            <xs:documentation>The number of individual units in the unit packet.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="UnitPacketPictureFile" type="AttachmentRef" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The picture of the unit packet. The picture must be clear enough to view details and to assist
                                in the identification of unique products.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="AnnualSalesDataList" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The supplementary market data available to the submitter. To be updated in case new data
                                become available.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="AnnualSalesData" type="EcigAnnualSalesData" maxOccurs="unbounded">
                                    <xs:annotation>
                                        <xs:documentation>The list of annual sales data.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="EcigAnnualSalesData">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette annual sales data.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AnnualSalesData">
                <xs:sequence>
                    <xs:element name="SalesMode" type="AttachmentRef" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Description of mode of sale of the product.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="EcigIngredient">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette ingredient.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Ingredient">
                <xs:sequence>
                    <xs:element name="IdentificationRefillContainerCartridge" type="String300" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Identification of the refill container/cartridge.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="EcigEmission">
        <xs:annotation>
            <xs:documentation>Defines an e-cigarette emission.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Emission">
                <xs:sequence>
                    <xs:element name="AdditionalProducts" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>If the product requires an additional product(s) for use, the EC-ID of the additional
                                product(s) used to carry out the tests must be provided. If EC_ID of the additional product(s) is not known
                                to the submitter, brand and subtype name(s) as well as Member State(s) where product is placed on the market
                                shall at least be provided.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ProductIdentification" type="ProductIdentification" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ProductCombination" type="String500" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>If the product contains more than one item or more than one combination of an e-cigarette or
                                refill container, specification of the item or combination used to measure the emission.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:sequence>
                        <xs:element name="Name" type="EmissionName">
                            <xs:annotation>
                                <xs:documentation>Name of the emission produced during the testing of the product.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence>
                        <xs:element name="NameOther" type="String100" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Other name of the emission produced during the testing of the product.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="EmissionName">
        <xs:annotation>
            <xs:documentation>Defines an emission name.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="EmissionNameEnum">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the emission name is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="EmissionNameEnum">
        <xs:annotation>
            <xs:documentation>The list of emission names.</xs:documentation>
            <xs:appinfo>
                <jaxb:typesafeEnumClass/>
